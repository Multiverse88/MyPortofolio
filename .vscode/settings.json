{
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "css.customData": [
    ".vscode/css_custom_data.json"
  ],
  "tailwindCSS.includeLanguages": {
    "plaintext": "html",
    "javascript": "javascript",
    "typescript": "typescript"
  },
  "tailwindCSS.experimental.classRegex": [
    "tw`([^`]*)",
    ["clsx\\(([^)]*)\\)", "'([^']*)'"],
    ["classnames\\(([^)]*)\\)", "'([^']*)'"],
    ["cva\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"],
    ["cn\\(([^)]*)\\)", "\"([^\"]*)\""]
  ],
  "files.associations": {
    "*.css": "tailwindcss"
  }
}